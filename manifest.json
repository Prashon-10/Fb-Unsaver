{
    "manifest_version": 3,
    "name": "FB-Unsaver",
    "version": "1.0",
    "description": "Automatically unsave items from Facebook collections.",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "host_permissions": [
      "*://*.facebook.com/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_title": "Run FB-Unsaver",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    }
  }
  